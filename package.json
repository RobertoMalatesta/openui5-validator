    "validate",
    "validator",
    "validation"
  ],
  "main": "dist/openui5/validator/library-preload.js",
  "dependencies": {
    "@openui5/sap.m": "^1.56.4",
    "@openui5/sap.ui.core": "^1.61.2",
    "@openui5/themelib_sap_fiori_3": "^1.61.2",
    "ajv": "^6.5.5"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^2.1.0",
    "karma-openui5": "^0.2.3",
    "karma-qunit": "^2.1.0",
    "qunit": "^2.9.1"
  },
  "scripts": {
    "start": "ui5 serve",
    "build": "ui5 build",
